{
  "name": "@domql/tags",
  "version": "2.3.56",
  "license": "MIT",
  "type": "module",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "dist/esm/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "index.js",
  "files": [
    "*.js",
    "dist"
  ],
  "scripts": {
    "copy:package:cjs": "cp ../../package-cjs.json dist/cjs",
    "build:esm": "npx esbuild *.js --target=es2020 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild *.js --target=node16 --format=cjs --outdir=dist/cjs && copy:package:cjs",
    "build:iife": "npx esbuild *.js --target=node16 --format=iife --outdir=dist/iife",
    "prepublish": "rm -rf dist && yarn build:esm && yarn build:cjs && yarn build:iife"
  },
  "gitHead": "d3e2a8cef1a70c00f05aaf7e91389ce046365df4"
}
