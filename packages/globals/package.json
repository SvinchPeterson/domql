{
  "name": "@domql/globals",
  "version": "2.3.55",
  "license": "MIT",
  "type": "module",
  "module": "dist/esm/index.js",
  "unpkg": "dist/iife/index.js",
  "jsdelivr": "dist/iife/index.js",
  "main": "dist/esm/index.js",
  "exports": "./dist/cjs/index.js",
  "source": "index.js",
  "files": [
    "*.js",
    "dist"
  ],
  "scripts": {
    "build:esm": "npx esbuild *.js --target=es2020 --format=esm --outdir=dist/esm",
    "build:cjs": "npx esbuild *.js --target=node16 --format=cjs --outdir=dist/cjs",
    "build:iife": "npx esbuild *.js --target=node16 --format=iife --outdir=dist/iife",
    "prepublish": "rm -rf dist && yarn build:esm && yarn build:cjs && yarn build:iife"
  },
  "gitHead": "66641a0ee9c94ecbe631b7b92e2018ad18af387b"
}
